{
  "name": "tracklog",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build:css & npm run build:js",
    "build:css": "node-sass --output-style compressed css/tracklog.scss public/tracklog.css",
    "build:js": "browserify -t babelify js/tracklog.js > public/tracklog.js",
    "production:build": "npm run build:css & npm run production:build:js",
    "production:build:js": "NODE_ENV=production browserify -t babelify js/tracklog.js | uglifyjs -m -c --screw-ie8 > public/tracklog.js"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "browserify": "^10.2.6",
    "babelify": "^7.2.0",
    "uglify-js": "^2.6.1",
    "node-sass": "^3.4.2"
  },
  "dependencies": {
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "flux": "^2.1.1",
    "bootstrap-sass": "^3.3.6",
    "leaflet": "^0.7.7",
    "whatwg-fetch": "^0.10.1",
    "immutable": "^3.7.6",
    "highcharts": "^4.2.1",
    "classnames": "^2.2.3"
  }
}
